{% extends 'base.html.twig' %}

{% block title %}MemberUser index{% endblock %}

{% block body %}
    <h1>MemberUser index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Telephone</th>
                <th>Email</th>
                <th>FirstName</th>
                <th>Surname</th>
                <th>Username</th>
                <th>Roles</th>
                <th>Password</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for member_user in member_users %}
            <tr>
                <td>{{ member_user.id }}</td>
                <td>{{ member_user.telephone }}</td>
                <td>{{ member_user.email }}</td>
                <td>{{ member_user.firstName }}</td>
                <td>{{ member_user.surname }}</td>
                <td>{{ member_user.username }}</td>
                <td>{{ member_user.roles ? member_user.roles|json_encode : '' }}</td>
                <td>{{ member_user.password }}</td>
                <td>
                    <a href="{{ path('member_user_show', {'id': member_user.id}) }}">show</a>
                    <a href="{{ path('member_user_edit', {'id': member_user.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="9">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('member_user_new') }}">Create new</a>
{% endblock %}
